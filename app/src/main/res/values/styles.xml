<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Base application theme. -->
      <style name="AppTheme" parent="Base.Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    
    <style name="AppTheme.Button" parent="Widget.AppCompat.Button">
        <item name="android:radius">2dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:colorButtonNormal" tools:targetApi="lollipop">#A86594</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">18dp</item>
        <item name="android:layout_marginBottom">42dp</item>
    </style>

    <style name="AppTheme.ButtonHighlighted" parent="Widget.AppCompat.Button">
        <item name="android:radius">2dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:colorButtonNormal" tools:targetApi="lollipop">#FFAEEB</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">18dp</item>
        <item name="android:layout_marginBottom">42dp</item>
    </style>

    <style name="spinnerItemStyle" parent="android:Widget.TextView.SpinnerItem">
    </style>

    <style name="spinnerDropDownItemStyle" parent="android:TextAppearance.Material.Widget.TextView.SpinnerItem">
    </style>


</resources>
